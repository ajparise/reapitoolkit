<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.web>
    <compilation debug="true" />
  </system.web>
  <!-- When deploying the service library project, the content of the config file must be added to the host's 
  app.config file. System.Configuration does not support config files for libraries. -->
  <system.serviceModel>
    <services>
      <service behaviorConfiguration="REDAPServiceLibrary.Service1Behavior"
        name="REDAPServiceLibrary.Service1">
        <endpoint address="" binding="wsHttpBinding" contract="REDAPServiceLibrary.IService1">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8731/Design_Time_Addresses/REDAPServiceLibrary/Service1/" />
          </baseAddresses>
        </host>
      </service>
      <service behaviorConfiguration="Parise.RaisersEdge.Toolkit.Services.ImportServiceBehavior"
        name="Parise.RaisersEdge.Toolkit.Services.ImportService">
        <endpoint address="" binding="wsHttpBinding" contract="Parise.RaisersEdge.Toolkit.Services.IImportService">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8731/Design_Time_Addresses/Parise.RaisersEdge.Toolkit.Services/ImportService/" />
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="REDAPServiceLibrary.Service1Behavior">
          <serviceMetadata httpGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="false" />
        </behavior>
        <behavior name="Parise.RaisersEdge.Toolkit.Services.ImportServiceBehavior">
          <serviceMetadata httpGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="false" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>
